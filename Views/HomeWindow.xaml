<Window x:Class="Deno.Views.HomeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Denomination"  WindowState="Maximized">
	<Grid>
		<TabControl>
			<TabItem x:Name="HomeTab" Header="Home">
				<Frame x:Name="HomeFrame" NavigationUIVisibility="Hidden"/>
			</TabItem>
			<TabItem x:Name="ManageTab" Header="Manage">
				<Frame x:Name="ManageFrame" NavigationUIVisibility="Hidden"/>
			</TabItem>
			<TabItem x:Name="ConfigTab" Header="Config">
				<Frame x:Name="ConfigFrame" NavigationUIVisibility="Hidden"/>
			</TabItem>
		</TabControl>
	</Grid>
</Window>
